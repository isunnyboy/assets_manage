<openerp>
    <data>
        <!--************************设备信息 Action Begin ************************************************-->
        <!--库存设备信息 所有人可见-->
        <record id="instore_assets_info_action" model="ir.actions.act_window" >
            <field name="name">库存设备</field>
            <field name="res_model">assets_management.equipment_info</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form,graph</field>
            <field name ="domain">[('dev_state','=',u'库存')]</field>
            <field name="view_id" ref="instore_assets_management_form_view"/>
        </record>
        <record id="instore_assets_info_action_tree" model="ir.actions.act_window.view">
            <field name="view_mode">tree</field>
               <field name="sequence" eval="1"/>
            <field name="view_id" ref="instore_assets_management_tree_view" />
            <field name="act_window_id" ref="instore_assets_info_action" />
        </record>
        <record id="instore_assets_info_action_form" model="ir.actions.act_window.view">
            <field name="view_mode">form</field>
            <field name="sequence" eval="2"/>
            <field name="view_id" ref="instore_assets_management_form_view" />
            <field name="act_window_id" ref="instore_assets_info_action" />
        </record>

        <!--待入库设备信息===设备提交人、备件管理员-->
        <record id="tostoreage_assets_info_action" model="ir.actions.act_window" >
            <field name="name">待入库设备</field>
            <field name="res_model">assets_management.equipment_info</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name ="domain">[('user_id','=',uid),('dev_state','=',u'待入库')]</field>
            <field name="view_id" ref="tostore_assets_management_form_view"/>
        </record>
        <record id="tostoreage_assets_info_action_tree" model="ir.actions.act_window.view">
            <field name="view_mode">tree</field>
            <field name="sequence" eval="1"/>
            <field name="view_id" ref="tostore_assets_management_tree_view" />
            <field name="act_window_id" ref="tostoreage_assets_info_action" />
        </record>
        <record id="tostoreage_assets_info_action_form" model="ir.actions.act_window.view">
            <field name="view_mode">form</field>
            <field name="sequence" eval="2"/>
            <field name="view_id" ref="tostore_assets_management_form_view" />
            <field name="act_window_id" ref="tostoreage_assets_info_action" />
        </record>

        <!--全部设备====备件管理员-->
        <record id="all_store_assets_info_action" model="ir.actions.act_window" >
            <field name="name">所有设备</field>
            <field name="res_model">assets_management.equipment_info</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name ="domain">[('dev_state','!=',u'库存'),('dev_state','!=','流程中'),('dev_state','!=','待入库')]</field>
            <field name="view_id" ref="all_store_assets_management_form_view"/>
        </record>
        <record id="all_store_assets_info_action_tree" model="ir.actions.act_window.view">
            <field name="view_mode">tree</field>
            <field name="sequence" eval="1"/>
            <field name="view_id" ref="all_store_assets_management_tree_view" />
            <field name="act_window_id" ref="all_store_assets_info_action" />
        </record>
        <record id="all_store_assets_info_action_form" model="ir.actions.act_window.view">
            <field name="view_mode">form</field>
            <field name="sequence" eval="2"/>
            <field name="view_id" ref="all_store_assets_management_form_view" />
            <field name="act_window_id" ref="all_store_assets_info_action" />
        </record>
        <!--************************设备信息 Action END ************************************************-->

        <!--************************我的请求信息 Action BEGIN ************************************************-->
        <!--我的请求===所有人都看见的导航==申请人只能查看到自己的请求单-->
        <record id="my_assets_storing_action" model="ir.actions.act_window" >
            <field name="name">我的入库请求</field>
            <field name="res_model">assets_management.equipment_storage</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name ="domain">[('user_id','=',uid)]</field>
        </record>
        <record id="my_assets_storing_tree_action" model="ir.actions.act_window.view">
            <field name="view_mode">tree</field>
            <field name="view_id" ref="my_assets_storage_tree_view" />
            <field name="act_window_id" ref="my_assets_storing_action" />
        </record>
        <record id="my_assets_storing_from_action" model="ir.actions.act_window.view">
            <field name="view_mode">form</field>
            <field name="view_id" ref="my_assets_storage_form_view" />
            <field name="act_window_id" ref="my_assets_storing_action" />
        </record>
        <!--************************我的请求信息 Action END ************************************************-->


        <!--************************我的代办、已办信息 Action BEGIN ************************************************-->
        <!--入库信息 Action-->
        <!--入库信息 待办  Action-->
        <record id="toDo_assets_storing_action" model="ir.actions.act_window" >
            <field name="name">入库申请待处理</field>
            <field name="res_model">assets_management.equipment_storage</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name ="domain">[('approver_id','=',uid),('state','!=','done')]</field>
            <!--<field name="search_view_id" ref="asset_management_search_view"></field>-->
             <!-- <field name="view_id" ref="resource_cost_tree_view"/>-->
            <!--<field name ="domain">[(uid,'in','user_toApprove')]</field>-->
            <!--<field name ="domain">[('user_toApproveChar','ilike',uid)]</field>-->
        </record>
        <record id="toDo_assets_storing_tree_action" model="ir.actions.act_window.view">
            <field name="view_mode">tree</field>
            <field name="view_id" ref="assets_storage_tree_view" />
            <field name="act_window_id" ref="toDo_assets_storing_action" />
        </record>
        <record id="toDo_assets_storing_from_action" model="ir.actions.act_window.view">
            <field name="view_mode">form</field>
            <field name="view_id" ref="toDo_assets_storage_form_view" />
            <field name="act_window_id" ref="toDo_assets_storing_action" />
        </record>
        <!--入库信息 已办  Action-->
        <record id="done_assets_storing_action" model="ir.actions.act_window" >
            <field name="name">入库申请已处理</field>
            <field name="res_model">assets_management.equipment_storage</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name ="domain">[('store_exam_ids.approver_id','=',uid)]</field>
            <!--<field name ="domain">[('user_id','=',uid),('state','!=','demander'),('state','in',[user_haveApproveChar.split()])]</field>-->
        </record>
        <record id="done_assets_storing_tree_action" model="ir.actions.act_window.view">
            <field name="view_mode">tree</field>
             <field name="sequence" eval="1"/>
            <field name="view_id" ref="assets_storage_tree_view" />
            <field name="act_window_id" ref="done_assets_storing_action" />
        </record>
        <record id="done_assets_storing_from_action" model="ir.actions.act_window.view">
            <field name="view_mode">form</field>
            <field name="sequence" eval="2"/>
            <field name="view_id" ref="done_assets_storage_form_view" />
            <field name="act_window_id" ref="done_assets_storing_action" />
        </record>
    <!--************************我的代办、已办信息 Action END ************************************************-->
        <!--*********************设备信息 Begin*********************-->
        <menuitem id="main_assets_management_menu" name="Assets management" sequence="1"/>
        <menuitem id="device_menu" name="device_information" parent="main_assets_management_menu" sequence="2"/>

        <!--库存信息===所有人都看见的导航-->
        <menuitem id="in_store_device_menu" name="in store device" parent="device_menu" action="instore_assets_info_action"/>
        <!--待入库设备===设备提交人、备件管理员-->
        <menuitem id="to_storeage_device_menu" name="to storage Device" parent="device_menu" action="tostoreage_assets_info_action"/>
        <!--&lt;!&ndash;全部借用设备====备件管理员&ndash;&gt;-->
        <!--<menuitem id="all_lent_device_menu" name="all lent device" parent="device_menu" action="store_assets_info_action" sequence="5"/>-->
        <!--&lt;!&ndash;全部领用设备====备件管理员&ndash;&gt;-->
        <!--<menuitem id="all_out_device_menu" name="all out Device" parent="device_menu" action="store_assets_info_action" sequence="5"/>-->
        <!--&lt;!&ndash;我借用设备====备件借用人&ndash;&gt;-->
        <!--<menuitem id="out_device_menu" name="my out Device" parent="device_menu" action="store_assets_info_action" sequence="5"/>-->
        <!--&lt;!&ndash;我领用设备====备件领用人&ndash;&gt;-->
        <!--<menuitem id="out_device_menu" name="my out Device" parent="device_menu" action="store_assets_info_action" sequence="5"/>-->
        <!--全部设备====备件管理员-->
        <menuitem id="all_device_menu" name= "all Device" parent="device_menu" action="all_store_assets_info_action"/>
        <!--*********************设备信息 END*********************-->

        <!--我的请求===所有人都看见的导航==申请人只能查看到自己的请求单-->
        <menuitem id="my_Request_menu" name="My Request Menu"  parent="main_assets_management_menu" sequence="6"/>
        <menuitem id="my_storing_menu" name="storing Menu" parent="my_Request_menu" action="my_assets_storing_action" />

        <!--入库请求===审批人员可见的导航==审批人员只能查看到自己待处理、已处理的入库请求单-->
        <menuitem id="devs_storing_menu" name="Devs Storing Request Menu"  parent="main_assets_management_menu" sequence="7"/>
        <menuitem id="toDo_storing_menu" name="Todo Storing Menu" parent="devs_storing_menu" action="toDo_assets_storing_action" />
        <menuitem id="done_storing_menu" name="Done Storing Menu" parent="devs_storing_menu" action="done_assets_storing_action" />

    </data>
</openerp>